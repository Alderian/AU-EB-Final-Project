{
  "name": "au-eb-final-project",
  "version": "0.1.0",
  "description": "Escrow with a Governance voting system, but it has some interesting implementation spin. The project aims to join VC and Unicorns together to make good ideas real!",
  "main": "index.js",
  "scripts": {
    "build": "npx hardhat compile",
    "format": "prettier --check \"**/*.{ts,js,sol}\"",
    "format:fix": "prettier --write \"**/*.{ts,js,sol}\"",
    "solhint": "solhint -f table contracts/**/*.sol",
    "check": "npx hardhat check",
    "compile": "npx hardhat compile",
    "test": "npx hardhat test --typecheck",
    "chain": "npx hardhat node",
    "deploy": "npx hardhat deploy",
    "compile-contract-types": "npx hardhat compile",
    "postinstall": "yarn compile-contract-types"
  },
  "repository": "git+https://github.com/Alderian/AU-EB-Final-Project.git",
  "keywords": [
    "web3",
    "Alchemy-University",
    "hardhat"
  ],
  "author": "Alderian Annatar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alderian/AU-EB-Final-Project/issues"
  },
  "homepage": "https://github.com/Alderian/AU-EB-Final-Project#readme",
  "devDependencies": {
    "@ethersproject/abi": "^5.4.7",
    "@ethersproject/providers": "^5.4.7",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-solhint": "^3.0.0",
    "@openzeppelin/contracts": "^4.8.2",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "@types/node": ">=12.0.0",
    "chai": "^4.2.0",
    "dotenv": "^16.0.3",
    "ethers": "^5.4.7",
    "hardhat": "^2.12.6",
    "hardhat-deploy": "^0.11.22",
    "hardhat-gas-reporter": "^1.0.8",
    "prettier": "^2.8.3",
    "prettier-plugin-solidity": "^1.1.1",
    "solhint": "^3.3.8",
    "solidity-coverage": "^0.8.0",
    "ts-node": ">=8.0.0",
    "typechain": "^8.1.0",
    "typescript": ">=4.5.0"
  }
