{
    "compilerOptions": {
        "target": "es2020",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "paths": {
            "typechain-types": ["./typechain-types"]
        }
    },
    "include": ["./hardhat.config.ts", "**/*.ts", "**/*.tsx"],
    "exclude": ["node_modules"]
}
